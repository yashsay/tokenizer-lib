{
  "name": "tokenizer-lib",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "tokens:build": "style-dictionary build --config style-dictionary.config.mjs",
    "tokens:clean": "rimraf dist/tokens",
    "storybook": "npm run tokens:build && storybook dev -p 6006",
    "build-storybook": "npm run tokens:build && storybook build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:storybook/recommended"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-docs": "^9.1.5",
    "@storybook/addon-onboarding": "^9.1.5",
    "@storybook/preset-create-react-app": "^9.1.5",
    "@storybook/react-webpack5": "^9.1.5",
    "style-dictionary": "^4.0.0",
    "rimraf": "^6.0.1",
    "eslint-plugin-storybook": "^9.1.5",
    "prop-types": "^15.8.1",
    "storybook": "^9.1.5",
    "webpack": "^5.101.3"
  }
}
